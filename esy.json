{
  "name": "reuniverse",
  "version": "0.1.0",
  "description": "Reason Documentation Universe",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build",
    "release": {
      "bin": ["reuniverse"],
      "includePackages": ["root", "esy-gmp", "@opam/nocrypto"]
