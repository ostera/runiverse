  "scripts": {
    "start": "./_build/native/src/app.native",
    "buildexample": "esy rebel",
    "editor": "esy $EDITOR .",
    "whereisocamlmerlin": "esy which ocamlmerlin-reason",
    "mlre": "pbpaste | esy refmt -parse ml -print re -use-stdin true -is-interface-pp false",
    "reml": "pbpaste | esy refmt -parse re -print ml -use-stdin true -is-interface-pp false"
  },
  "dependencies": {
    "tgls-actual": "github:bsansouci/tgls#esy",
    "@opam-alpha/topkg": "*",
    "tsdl-actual": "github:bsansouci/tsdl#esy",
    "rebel": "github:bsansouci/rebel#esy",
    "reason": "github:facebook/reason#esy",
    "reglinterface": "^0.1.19"
  },
  "esy": {
    "build": [
      "gcc -I`ocamlc -where` -c src/soil_wrapper.c -o $cur__lib/libsoil_wrapper.a",
      "make",
      "node setcurrentdir.js"
    ],
    "exportedEnv": {
      "PKG_CONFIG_PATH": {
        "scope": "global",
        "val": "$reglnative__lib:$PKG_CONFIG_PATH"
      }
    },
    "buildsInSource": true
  },
  "rebel": {
    "targets": [
      {
        "target": "native",
        "engine": "native",
        "entry": "src/example.re",
        "unstable_flags": {
          "compile": "-package tgls.tgl3,tsdl -I $cur__target_dir/native/reglnative",
          "link": "-linkpkg -package tgls.tgl3,tsdl -cclib \"$(pkg-config --libs $cur__lib/reglnative.pc)\""
        }
      }
    ]
  },
  "devDependencies": {
    "lerna": "2.0.0-beta.30",
    "ocaml": "github:esy-ocaml/ocaml#esy",
